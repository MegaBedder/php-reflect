    },
    "suggest": {
        "symfony/finder": ">=2.3",
        "symfony/class-loader": ">=2.3",
        "pear-phpunit/PHPUnit": ">=3.7"
    },
    "authors": [
        {
            "name": "Laurent Laville",
            "email": "pear@laurent-laville.org",
            "homepage": "https://github.com/llaville",
            "role": "Lead"
        }
    ],
    "autoload": {
        psr-0": {
            "Bartlett\\": "src/"
            "Bartlett\\Tests\\Reflect\\": "tests/"
        }
    }
}