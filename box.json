{
    "alias": "phpreflect.phar",
    "chmod": "0755",
    "directories": ["src"],
    "extract": true,
    "compression": "GZ",
    "files": [
        "LICENSE"
    ],
    "finder": [
        {
            "name": "*.php",
            "in": "vendor/PHP-Parser"
        },
        {
            "name": "*.php",
            "notPath": "/Tests/",
            "in": "vendor/Symfony"
        },
        {
            "name": "*.php",
            "in": "vendor/PHPUnit"
        }
    ],
    "metadata": [
        {
            "vendor": [
                {
                    "nikic": [
                        {
                            "PHP-Parser": "1.0.0-dev+gh95"
                        }
                    ],
                    "symfony": [
                        {
                            "classloader": "2.4.2",
                            "console": "2.4.2",
                            "eventdispatcher": "2.4.2",
                            "finder": "2.4.2"
                        }
                    ],
                    "phpunit": [
                        {
                            "php-timer": "1.0.5"
                        }
                    ]
                }
            ]
        }
    ],
    "git-commit": "git_commit",
    "git-version": "package_version",
    "main": "bin/reflect",
    "output": "phpreflect.phar",
    "stub": true,
    "web": false
}
