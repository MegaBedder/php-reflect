{
    "alias": "phpreflect.phar",
    "chmod": "0755",
    "directories": ["src"],
    "extract": true,
    "compression": "GZ",
    "files": [
        "LICENSE",
        "vendor/autoload.php"
    ],
    "finder": [
        {
            "name": "*.php",
            "in": "vendor/composer"
        },
        {
            "name": "*.php",
            "in": "vendor/nikic/php-parser/lib"
        },
        {
            "name": "*.php",
            "notPath": "/Tests/",
            "in": "vendor/symfony"
        },
        {
            "name": "*.php",
            "in": "vendor/phpunit/php-timer"
        }
    ],
    "metadata": [
        {
            "vendor": [
                {
                    "nikic": [
                        {
                            "php-parser": "1.0.0-dev"
                        }
                    ],
                    "symfony": [
                        {
                            "console": "2.4.4",
                            "event-dispatcher": "2.4.4",
                            "finder": "2.4.4"
                        }
                    ],
                    "phpunit": [
                        {
                            "php-timer": "1.0.5"
                        }
                    ]
                }
            ]
        }
    ],
    "git-commit": "git_commit",
    "git-version": "package_version",
    "main": "bin/reflect",
    "output": "phpreflect-@git-version@.phar",
    "stub": "phar-stub.php",
    "web": false
}
